{
  "name": "parking-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:prod": "typeorm",
    "migration:run:prod": "typeorm migration:run -d dist/config/data-source.js",
    "migrate": "pnpm run build && pnpm run migration:run:prod",
    "migration:run": "pnpm run typeorm migration:run -d src/config/data-source.ts",
    "migration:revert": "pnpm run typeorm migration:revert -d src/config/data-source.ts",
    "migration:generate": "pnpm run typeorm migration:generate -d src/config/data-source.ts",
    "migration:run:test": "pnpm run typeorm migration:run -d src/config/test-data-source.ts",
    "migration:revert:test": "pnpm run typeorm migration:revert -d src/config/test-data-source.ts",
    "migration:generate:test": "pnpm run typeorm migration:generate -d src/config/test-data-source.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "all-e2e": "jest --config test/jest-e2e.json",
    "acceso-logs-e2e": "jest --config test/jest-e2e.json test/e2e/casos/acceso-logs.e2e-spec.ts",
    "actualizacion-usuarios-e2e": "jest --config test/jest-e2e.json test/e2e/casos/actualizacion-usuarios.e2e-spec.ts --runInBand",
    "consulta-ocupacion-e2e": "jest --config test/jest-e2e.json test/e2e/casos/consulta-ocupacion.e2e-spec.ts",
    "reserva-plaza-e2e": "jest --config test/jest-e2e.json test/e2e/casos/reserva-plaza.e2e-spec.ts",
    "auth-integration-e2e": "jest --config test/jest-e2e.json test/e2e/integration/auth-integration.e2e-spec.ts",
    "auth-e2e": "jest --config test/jest-e2e.json test/e2e/auth.e2e-spec.ts",
    "ocupacion-e2e": "jest --config test/jest-e2e.json test/e2e/ocupacion.e2e-spec.ts",
    "reservas-e2e": "jest --config test/jest-e2e.json test/e2e/reservas.e2e-spec.ts",
    "user-management-e2e": "jest --config test/jest-e2e.json test/e2e/user-management.e2e-spec.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "exceljs": "^4.4.0",
    "json2csv": "6.0.0-alpha.2",
    "mongoose": "^8.17.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@shelf/jest-mongodb": "^5.2.2",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/json2csv": "^5.0.7",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "dotenv": "17.2.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "mongodb": "6.19.0",
    "mongodb-memory-server": "^8.16.1",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
